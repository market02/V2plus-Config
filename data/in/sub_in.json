[
  {
    "URL": "https://openclash.cc/",
    "steps": [
      {
        "type": "click",
        "xpath": "//*[@id='blog-list']/div[2]/div[1]/div[2]/div[1]/a"
      },
      {
        "type": "search",
        "rules": "v2ray订阅链接"
      },
      {
        "type": "extract",
        "rules": "https://node.openclash.cc/uploads/*/*/*.txt",
        "next": {
          "type": "open",
          "next": {
            "type": "decode",
            "encoding": "base64"
          }
        },
        "output": "single",
        "recycle": "multi-connect"
      }
    ]
  },
  {
