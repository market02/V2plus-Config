  {
    "URL": "https://surgenode.github.io/free-nodes/",
    "steps": [
      {
        "type": "click",
        "selectors": [
          "body > div.space-medium > div > div > div.col-md-9.xcblog-blog-list > div:nth-child(1) > div.col-md-9 > a",
          "div.xcblog-blog-list > div:first-child a",
          "div.col-md-9 > a:first-child",
          "//div[contains(@class,'xcblog-blog-list')]//div[1]//a",
          "//div[contains(@class,'col-md-9')]//a[1]",
          "a[class*='blog']:first-child"
        ]
      },
      {
        "type": "search",
        "rules": "v2ray订阅链接"
      },
      {
        "type": "extract",
        "rules": "https://node.freeclashnode.com/uploads/*/*/*.txt",
        "next": {
          "type": "open",
          "next": {
            "type": "decode",
            "encoding": "base64"
          }
        },
